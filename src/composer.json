{
    "name": "jmartz-api",
    "description": "Jmartz",
    "keywords": [
        "jmartz",
        "laravel",
        "lumen",
        "api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "^5.7.0",
        "league/flysystem": "^1.0",
        "jonathan-martz/lumen-exeception-to-json": "^0.2.8",
        "jonathan-martz/lumen-borat-controller": "^0.2",
        "phan/phan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
