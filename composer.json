{
    "name": "jmartz-api",
    "description": "Jmartz",
    "keywords": [
        "jmartz",
        "laravel",
        "lumen",
        "api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "^6.2.0",
        "league/flysystem": "^1.0",
        "jonathan-martz/lumen-exeception-to-json": "^0.3.1",
        "jonathan-martz/lumen-borat-controller": "^0.2",
        "phan/phan": "^2.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~8.4",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
