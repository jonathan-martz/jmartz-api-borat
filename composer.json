{
    "name": "jmartz-api",
    "description": "Jmartz",
    "keywords": [
        "jmartz",
        "laravel",
        "lumen",
        "api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.7.*",
        "vlucas/phpdotenv": "~2.2",
        "league/flysystem": "^1.0",
        "jonathan-martz/lumen-exeception-to-json": "@dev",
        "jonathan-martz/lumen-borat-controller": "@dev",
        "phan/phan": "^1.1",
        "ext-json": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://api-borat.jmartz.de/"
        }
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
